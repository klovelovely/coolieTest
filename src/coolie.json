{
  "js": {
    "src": [
      "./static/js/app.js",
      "./static/js/app/**/*.js",
      "./static/js/plugin/**/*.js"
    ],
    "coolie-config.js": "./static/js/coolie-config.js",
    "dest": "./static/js/"
  },
  "css": {
    "src": [
      "./static/css/*.css"
    ],
    "dest": "./static/css/",
    "minify": {
      "compatibility": "ie7"
    }
  },
  "html": {
    "src": [
      "./index.html",
      "./views/**/*.html"
    ],
    "minify": true
  },
  "resource": {
    "dest": "./static/res/",
    "minify": true
  },
  "copy": [],
  "dest": {
    "dirname": "../dest/",
    "host": ""
  }
}